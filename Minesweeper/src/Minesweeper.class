    public boolean boardComplete() {
        int nonTiles = 66;
        for (int r = 0; r < this.board.length; ++r) {
            for (int c = 0; c < this.board[r].length; ++c) {
                if (!this.board[r][c].getClicked()) continue;
                --nonTiles;
            }
        }
        return nonTiles <= 0;
    }

    public void gameWin() {
        this.subGUI.running = false;
        JOptionPane.showMessageDialog(new JFrame(), "You win! Your time is " + this.s + " seconds!");
        System.exit(0);
    }

