    private class TimeGUI
    extends JFrame {
        private Thread updater;
        private boolean running;

        public TimeGUI() {
            super("Time");
            this.running = true;
            this.setLayout(new GridLayout(2, 1));
            this.updater = new Thread(new timer());
            this.updater.start();
            this.add(Minesweeper.this.flagButton);
            this.add(Minesweeper.this.time);
        }

        private class timer
        implements Runnable {
            private timer() {
            }

            @Override
            public void run() {
                if (Minesweeper.this.firstClick) {
                    double elapsed = (double)(System.currentTimeMillis() - Minesweeper.this.start) / 1000.0;
                    elapsed = (double)Math.round(elapsed * 100.0) / 100.0;
                    Minesweeper.this.s = Double.toString(elapsed);
                    int index = Minesweeper.this.s.indexOf(".");
                    Minesweeper.this.s = Minesweeper.this.s.substring(0, index) + ":" + Minesweeper.this.s.substring(index + 1, Minesweeper.this.s.length());
                    Minesweeper.this.time.setText(Minesweeper.this.s);
                }
                try {
                    Thread.sleep(100L);
                }
                catch (InterruptedException interruptedException) {
                    // empty catch block
                }
                if (TimeGUI.this.running) {
                    this.run();
                }
            }
        }
    }

    private class MinesweeperTile {
        private boolean isMine;
        private boolean flag;
        private boolean clicked;
        private int adjacentMine;

        public MinesweeperTile() {
            this.isMine = false;
            this.adjacentMine = 0;
            this.clicked = false;
        }

        public MinesweeperTile(boolean m) {
            this.isMine = m;
            this.adjacentMine = 0;
            this.clicked = false;
        }

        public MinesweeperTile(boolean m, int a) {
            this.isMine = m;
            this.adjacentMine = a;
            this.clicked = false;
        }

        public void setToMine() {
            this.isMine = true;
        }

        public boolean getMine() {
            return this.isMine;
        }

        public void alternateFlag() {
            this.flag = !this.flag;
        }

        public boolean getFlag() {
            return this.flag;
        }

        public boolean getClicked() {
            return this.clicked;
        }

        public void setClicked() {
            this.clicked = true;
        }

        public int getAdjacent() {
            return this.adjacentMine;
        }

        public void addAdjacent() {
            ++this.adjacentMine;
        }

        public String toString() {
            if (this.getMine()) {
                return "*";
            }
            return Integer.toString(this.getAdjacent());
        }
    }
